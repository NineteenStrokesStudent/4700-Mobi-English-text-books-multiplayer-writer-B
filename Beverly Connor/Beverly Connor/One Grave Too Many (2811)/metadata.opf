        <dc:title>One Grave Too Many</dc:title>
        <dc:creator opf:file-as="Connor, Beverly" opf:role="aut">Beverly Connor</dc:creator>
        <dc:contributor opf:file-as="calibre" opf:role="bkp">calibre (0.7.35) [http://calibre-ebook.com]</dc:contributor>
